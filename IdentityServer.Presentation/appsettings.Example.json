{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityServerConnection": "Data Source=(local);Initial Catalog=IdentityServer;Integrated Security=True;TrustServerCertificate=true;Encrypt=False;MultipleActiveResultSets=true"
  },
  "JsonWebTokenSettings": {
    "Issuer": "https://yourIssuer.com",
    "Audience": "https://yourAudience.com",
    "SigningKey": "yourSigningKey",
    "ExpirationMinutes": 10
  }
}